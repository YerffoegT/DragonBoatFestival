-- game

function game_init()
	health = 50
	score = 0
	timer = 0
	maxenemies = 0
	spawnrate = 0
	update = game_update
	draw = game_draw
	instructions:new()
end

function game_update()
 enemiesmanager()
 
 if timer ~= 9000 then
 	timer+=1
 else
 	timer=1
 end
 
 --every 3 seconds increase enemy max
	if timer%120==0 then 
		maxenemies += 1
		timer = 1
	end
	
	--update zongzi
	zongziarray:update()
	--update enemies
	enemiesarray:update()
	passivearray:update()
	--update player
	player:update()
	--update animations
	animationarray:update()
end

function game_draw()
	cls()
	--draw background
	rectfill(0,0,127,23,12)
	rectfill(0,24,127,127,1)
	--draw mountains & water surface
	spr(0,0,0,16,4)
	--draw seafloor
	spr(224,0,112,16,2)
	--draw enemies
	enemiesarray:draw()
	passivearray:draw()
	--draw zongzi
	zongziarray:draw()
	--draw player
	player:draw()
	--draw animations
	animationarray:draw()
	
	--draw hud
	drawscore()
	drawhealth()
	
	--debug stuff
	--print(maxenemies,4,12)
	--print(#enemiesarray,4,18)
	--print(timermod,4,24)
	--print(timer%timermod==0,4,30)
end

--global variables

score = 0
health = 50
healthx = 128-50-4
healthy = 4

timer = 0

maxenemies = 0
spawnrate = 90

quyuan = {
x=48,
y=112,
w=5,
h=2,
}

function drawscore()
	color(7)
	print(score,4,4)
end

function drawhealth()
	rectfill(healthx, healthy, healthx+health, healthy+2,8)
	if health <50 then rectfill(healthx+health+1,healthy, (healthx+health)+(50-health), healthy+2,7) end
end

function losehealth(x)
	if x>health then health = 0
	else health=health-x
	end
	if health == 0 then
		--empty arrays
		enemiesarray:empty()
		zongziarray:empty()
		passivearray:empty()
		animationarray:empty()
		--go to game over screen
	 over_init()
	end
end


