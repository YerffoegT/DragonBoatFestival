--global functions

function copy(o)
  local c
  if type(o) == 'table' then
    c = {}
    for k, v in pairs(o) do
      c[k] = copy(v)
    end
  else
    c = o
  end
  return c
end

function collision(u,v)
	if (
		u.x <= v.x+((v.w*8)-1) and
		u.x+((u.w*8)-1) >= v.x and
		u.y <= v.y+((v.h*8)-1) and
		u.y+((u.h*8)-1) >= v.y
		) then return true 
		else return false 
	end
end

function size(x)
	return ((x*8)-1)
end

--array class

arrayclass = {}

function arrayclass:update()
	for i in all(self) do
  i:update()
 end
end

function arrayclass:draw()
	for i in all(self) do
  i:draw()
 end
end

function arrayclass:empty()
	for i in all(self) do
		del(self, i)
	end
end

--animations

animationarray = copy(arrayclass)

animation = {
	sp,
	x,
	y,
	w=1,
	h=1,
	hf=false,
	vf=false,
	timer,
}

function animation:update()
	if timer%3==0 then
 	if self.sp==self.timer then 
 		self:die()
 	else
 		self.sp+=1
 	end
 end
end

function animation:draw()
	spr(self.sp, self.x, self.y, self.w, self.h,self.hf,self.vf)
end

function animation:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 add(animationarray, o)
end

function animation:die()
	del(animationarray, self)
end

function hcentertext(s)
	return 64-#s*2
end

instructions = copy(animation)
	instructions.y=40
	instructions.text1 = "press z to drop food"
	instructions.text2 = "press left and right to move"
	instructions.timer = 300
	instructions.temppal = 1


function instructions:update()
	self.timer-=1
	if self.timer%8==0 then self.temppal = 10 end
	if self.timer%8==4 then	self.temppal = 5 end
	if self.timer==0 then self:die() end
end

function instructions:draw() 
	print(self.text1, hcentertext(self.text1), self.y, self.temppal)
	print(self.text2, hcentertext(self.text2), self.y+7, self.temppal)
end
