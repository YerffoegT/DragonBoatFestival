-- player

player = {
	sp=64,
	x=40,
	y=8,
	w=6,
	h=3,
}

function player:move()
	if btn(0) and self.x>0 then self.x-=2 end
	if btn(1) and self.x<(128-(self.w*8)) then self.x+=2 end
end

function player:dropfood()
	if #zongziarray<3 then
		sfx(0)
		zongzi:new({x=self.x+(((self.w-1)*8)/2), y=self.y+(self.h*8)})
	end
end

function player:update()
	self:move()
	if btnp(4) then self:dropfood() end
end

function player:draw()
	spr(self.sp, self.x, self.y, self.w, self.h)
end

zongziarray=copy(arrayclass)

zongzi = {
	sp=127,
	x,
	y,
	w=1,
	h=1,
}

function zongzi:die()
	del(zongziarray, self)
end

function zongzi:update()
