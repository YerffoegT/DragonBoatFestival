-- enemies

fishspawnheight = 40
shrimpspawnheight = 96
crabspawnheight = 116

enemytimer = 1
timermod = 90

function enemiesmanager()
	enemytimer += 1
	switchnum = flr(rnd(6))
	switchnum2 = flr(rnd(2))
	if enemytimer%timermod==0 and #enemiesarray < maxenemies then
		enemytimer = 1
		timermod = 15+flr(75 * 10^((#enemiesarray/maxenemies)-1) + (0.1*(#enemiesarray/maxenemies)-0.1) )
		--spawn shrimp
		if switchnum==0 then
			--on left side or
			if switchnum2==0 then
				shrimp:new({x=-8,y=shrimpspawnheight+(flr(rnd(2))*8),speed=1,direc=1})
			--on right side
			elseif switchnum2==1 then
				shrimp:new({x=135,y=shrimpspawnheight+(flr(rnd(2))*8),hf=true,speed=1,direc=-1})
			end
		
		--spawn crab
		elseif switchnum>0 and switchnum<2 then
			--on left side or
			if switchnum2==0 then
				crab:new({x=-8,y=crabspawnheight,speed=1,direc=1})
			--on right side
			elseif switchnum2==1 then
				crab:new({x=135,y=crabspawnheight,hf=true,speed=1,direc=-1})
			end
			
		--spawn fish
		elseif switchnum>2 then
			--on left side or
			if switchnum2==0 then
				fish:new({x=-16,y=fishspawnheight+(flr(rnd(6))*8),speed=1,direc=1})
			--on right side
			elseif switchnum2==1 then
				fish:new({x=143,y=fishspawnheight+(flr(rnd(6))*8),hf=true,speed=1,direc=-1})
			end
		end
	end
end

enemiesarray = copy(arrayclass)

function enemiesarray:update()
	for i in all(self) do
  i:update()
  for j in all(zongziarray) do
  	if collision(i,j) then
  		i:die()
  		j:die()
  		sfx(4)
  		score += i.score
  	end
  end
 end
end

passivearray = copy(arrayclass)

--generic enemy class
enemy = {
	sp,
	x,
	y,
	w,
	h,
	hf=false,
	vf=false,
	speed,
	direc=1,
	score=100,
}

function enemy:move()
	--move
	self.x += self.speed * self.direc
	--if off screen, move down and flip direction
	if self.x < 0-(self.w*8) or self.x > 127+(self.w*8) then 
		self.direc = -self.direc
		self.hf = not self.hf
		self.speed = flr((self.y-16)/ 32)
		self.y+=(self.h*8)
	end
end

function enemy:damage()
	--if hit qu yuan then delete self
	if collision(self, quyuan) then 
		sfx(5)
		self:die()
		losehealth(10)
	end
end

function enemy:die()
	test = {sp=192,x=self.x,y=self.y,timer=194}
	animation:new(test)
	self.direc = -self.direc
	self.hf = not self.hf
	passive:new(self)
	del(enemiesarray, self)
end

function enemy:update()
	self:move()
	self:damage()
end

function enemy:draw()
	spr(self.sp, self.x, self.y, self.w, self.h,self.hf,self.vf)
end

function enemy:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 add(enemiesarray, o)
end

--shrimp enemy class
shrimp = copy(enemy)
shrimp.mod = 5

function shrimp:move()
	--move
	if timer%self.mod==0 then self.x += self.speed * self.direc end
	--if off screen, move down and flip direction
	if self.x < 0-(self.w*8) or self.x > 127+(self.w*8) then 
		self.direc = -self.direc
		self.hf = not self.hf
		self.y+=(self.h*8)
		if self.mod > 2 then self.mod -= 2 end
	end
end

function shrimp:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 o.sp = 129
 o.w=1
 o.h=1
 add(enemiesarray, o)
end

--crab enemy class
crab = copy(enemy)

function crab:move() 
	if timer%15==0 then self.x+= self.speed * self.direc end
end

function crab:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 o.sp = 128
 o.w=1
 o.h=1
 o.score = 200
 add(enemiesarray, o)
end

--fish enemy class
fish = copy(enemy)

function fish:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 o.sp = 130
 o.w = 2
 o.h = 1
 add(enemiesarray, o)
end

--passive class

passive = copy(enemy)

function passive:new(o)
	o = o or {}
 setmetatable(o or {}, self)
 self.__index = self
 add(passivearray, o)
end

function passive:damage()

end

function passive:move()
	--move
	self.x += self.speed * self.direc
	--if off screen, move down and flip direction
	if self.x < 0-(self.w*8) or self.x > 127+(self.w*8) then 
		del(passivearray, self)
	end
end
